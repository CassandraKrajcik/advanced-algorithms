{
  "metadata": [
    {
      "src": [
        {
          "files": ["Advanced.Algorithms.Docs.sln"],
          "src": "../src/"
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["**/*.yml"],
        "src": "obj/api",
        "dest": "api"
      },
      {
        "files": ["*.md"]
      }
    ],
    "resource": [
      {
        "files": [""]
      }
    ],
    "overwrite": "specs/*.md",
    "globalMetadata": {
      "_appTitle": "Advanced Algorithms",
      "_enableSearch": true
    },
    "dest": "../docs",
    "xrefService": ["https://xref.docs.microsoft.com/query?uid={uid}"]
  }
}